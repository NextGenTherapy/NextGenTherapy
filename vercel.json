{
  "redirects": [
    {
      "source": "/:path(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.nextgentherapy.co.uk"
        }
      ],
      "destination": "https://nextgentherapy.co.uk/:path",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Link",
          "value": "<https://nextgentherapy.co.uk/>; rel=\"canonical\""
        }
      ]
    }
  ]
}